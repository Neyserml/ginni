{
  "name": "angular-core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --environment=local",
    "start2": "ng serve  --host 0.0.0.0 --environment=dev2 --progress=true",
    "build:local": "ng build --aot --environment=local --progress=true",
    "build:dev": "ng build --aot --environment=dev --output-hashing=all --progress=true",
    "build:dev2": "ng build --aot --environment=dev2 --progress=true",
    "build:qa": "ng build --aot --environment=qa --progress=true",
    "build:qa2": "ng build --aot --environment=qa2 --progress=true",
    "build:int": "ng build --aot --environment=int --output-hashing=all --progress=true",
    "build:prod": "ng build --aot --environment=prod --output-hashing=all --progress=true",
    "build": "ng build --aot --progress=true",
    "test": "ng test --watch false --sourcemaps=false --environment=test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix --type-check",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "format": "prettier --write \"**/*.ts\"",
    "format:html": "prettier --write \"src/**/*.html\" --parser angular --html-whitespace-sensitivity \"css\"",
    "e2e": "ng e2e",
    "test:core": "ng test nyc --code-coverage --watch true --sourcemaps=false --environment=test",
    "test:debug": "ng test --code-coverage --watch true --sourcemaps=false --environment=test",
    "test:coverage": "cd coverage/ && http-server -o",
    "precommit": "lint-staged",
    "commit": "git-cz --no-verify",
    "mockup": "node data/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "@types/js-cookie": "^2.2.2",
    "angular-notifier": "^4.1.1",
    "angular-svg-icon": "^5.1.1",
    "angular-ui-carousel": "^0.1.10",
    "angular2-text-mask": "^9.0.0",
    "animated-scroll-to": "^1.3.1",
    "big.js": "^5.2.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.9",
    "crypto-js": "^3.1.9-1",
    "dropzone": "^5.5.1",
    "element-closest": "^2.0.2",
    "flexboxgrid": "^6.3.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.24.0",
    "ng2-tooltip-directive": "2.6.14",
    "ng5-slider": "^1.2.4",
    "ngx-pagination": "^3.3.1",
    "ngx-smart-modal": "^7.1.1",
    "normalize.css": "^8.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.10",
    "string.prototype.includes": "^1.0.0",
    "text-mask-addons": "^3.8.0",
    "web-animations-js": "^2.3.2",
    "whatwg-fetch": "^3.0.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.5.38",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "babel-plugin-istanbul": "^5.2.0",
    "body-parser": "^1.19.0",
    "codelyzer": "^4.0.1",
    "commitizen": "^2.10.1",
    "cors": "^2.8.4",
    "cz-conventional-changelog": "^2.1.0",
    "express": "^4.17.1",
    "formidable": "^1.2.1",
    "http-status": "^1.3.2",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-marbles": "^0.2.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "path-to-regexp": "^2.4.0",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2",
    "stylelint": "^9.10.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~2.5.3"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run format",
      "npm run test",
      "npm run build",
      "git add"
    ],
    "*.html": [
      "npm run format:html",
      "git add"
    ],
    "*.scss": [
      "npm run lint:styles",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "http://192.168.1.218:8084/repository/ginni-frontend/"
  }
}
